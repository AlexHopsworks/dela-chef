system {
    seed = <%= node.dela.seed %>
    port = <%= node.dela.port %>
    id = <%= node.dela.id %>
}
network {
   ipType = "PRIVATE"
}
stun {
    client {
        address {
            port1 = <%= node.dela.stun_port1 %>
            port2 = <%= node.dela.stun_port2 %>
        }
    }
}
nat.detection.stunView {
    server {
        ip = "<%= @stun1_ip %>"
        port1 = <%= node.dela.stun_server_port1 %>
        port2 = <%= node.dela.stun_server_port2 %>
        id = "<%= @stun1_id %>"
    }
    partner {
        ip = "<%= @stun2_ip %>"
        port1 = <%= node.dela.stun_server_port1 %>
        port2 = <%= node.dela.stun_server_port2 %>
        id = "<%= @stun2_id %>"
    }
}
webservice.server= "<%= node.dela.home %>/conf/config.yml"
report.dir = "<%= node.dela.home %>"
library.summary = "<%= node.dela.home %>/library.summary"
hops.library.baseEndpoint = <%= node.dela.hops.baseEndpoint %>

hops.mysql.user = <%= node.mysql.user %>
hops.mysql.password = <%= node.mysql.password %>